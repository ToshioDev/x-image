{
  "name": "tweet-image",
  "license": "MIT",
  "scripts": {
    "dev": "concurrently \"npm:server\" \"npm:start\" -c \"green,blue\"",
    "server": "nodemon --ignore .cache/ --ignore dist/ --ignore node_modules/ ./src/backend/server.js",
    "start": "parcel ./src/frontend/html/index.html --out-dir ./dist --public-url / --port 1271",
    "build": "parcel build ./src/frontend/html/index.html --out-dir ./dist --public-url / --no-source-maps"
  },
  "dependencies": {
    "babel-core": "^6.26.3",
    "babel-preset-react": "^6.24.1",
    "concurrently": "^5.3.0",
    "express": "^4.17.1",
    "nodemon": "^2.0.7",
    "parcel-bundler": "1.11.0",
    "puppeteer": "^5.5.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "rfv": "^0.0.36",
    "sass": "^1.32.5",
    "standard": "^16.0.3"
  }
}
